{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.web-admin"
  },
  "deploy": {
    "startCommand": "python web_admin_railway.py",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE"
  },
  "watchPaths": [
    "web-admin/**/*"
  ]
}
